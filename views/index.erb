<form action="/users" method="post">
    <input id="enter-username" name="username" placeholder="Enter username"
    alt="Text box to type a username in"/>
    <input id="enter-password" type="password" name="password" placeholder="Enter password"
    alt="Text box to type a password in"/>
    <input id="create-user" type="submit" name="create-user"
    value="Sign Up" alt="Button to submit new user registration"/>
    <input id="login-user" type="submit" name="login-user"
    value="Sign In" alt="Button to login existing users"/>
</form>
